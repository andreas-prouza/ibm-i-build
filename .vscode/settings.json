{
    "makefile.extensionOutputFolder": "./.vscode",
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "xxxxx.*$",
                "isAsync": true,
                "cmd": "xxxmake/scripts/sync2ibmi.sh"
            }
        ]
    },
    "command-runner.terminal.name": "Sync project",
    "command-runner.terminal.autoClear": true,
    "command-runner.terminal.autoFocus": true,
    "command-runner.commands": {
        "Sync source to IBM i": "make/scripts/cleanup.sh   &&   make/scripts/sync2ibmi.sh ",
        "Sync logs back": "make/scripts/cleanup.sh   &&   make/scripts/sync_back_from_ibmi.sh ",
        "Run build (summary output)": "make/scripts/cleanup.sh    &&   make/scripts/create_build_script.sh summary   &&   make/scripts/sync2ibmi.sh   &&   make/scripts/run_build.sh;   make/scripts/sync_back_from_ibmi.sh",
        "Run build (detailed output)": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh detailed   &&   make/scripts/sync2ibmi.sh   &&   make/scripts/run_build.sh;   make/scripts/sync_back_from_ibmi.sh",
        "Run build (debug)": "make/scripts/cleanup.sh debug   &&   make/scripts/create_build_script.sh debug   &&   make/scripts/sync2ibmi.sh debug   &&   make/scripts/run_build.sh debug;   make/scripts/sync_back_from_ibmi.sh debug",
        "Simulate build": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh simulate",
        "CleanUp Build": "make/scripts/cleanup.sh   &&   make clean",
        "Show Change Debug": "make/scripts/cleanup.sh debug   &&   make/scripts/create_build_script.sh show-change-debug  ",
        "Show Change Compile": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh schow-change-compile ",
        "Show Change Object":  "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh show-change-object",
        "Create Prod Object List": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh create-object-list ",
        "Create build script": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh summary ",
        "Reset Build Timestamp": "make/scripts/cleanup.sh   &&   make/scripts/reset.sh",
        "Create deployment": "make/scripts/create_deployment.sh",
        "Merge with production": "git pull && git -c 'core.editor=code --wait --reuse-window' merge main && git push && echo 'Done' && git rev-parse HEAD >> commit.log ",
        "Merge cancel": "git merge --abort"
    },
    "workbench.editor.enablePreview": false,
    "ShortcutMenuBar.beautify": false,
    "ShortcutMenuBar.navigateBack": false,
    "ShortcutMenuBar.navigateForward": false,
    "ShortcutMenuBar.openFilesList": false,
    "ShortcutMenuBar.toggleTerminal": false,
    "ShortcutMenuBar.showCommands": true,
    "ShortcutMenuBar.userButton01Command": "command-runner.run",
    "actionButtons": {
        "commands": [
            {
                "name": "ü™ü Show Change Object",
				"color": "orange",
				"useVsCodeApi": false,
                "command": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh show-change-object"
            },
            {
                "name": "‚ôªÔ∏è CleanUp",
				"color": "white",
				"useVsCodeApi": false,
                "command": "make/scripts/cleanup.sh  &&   make clean"
            },
            {
                "name": "$(run-all) Run build",
				"color": "white",
				"useVsCodeApi": false,
                "command": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh   &&   make/scripts/sync2ibmi.sh   &&   make/scripts/run_build.sh debug;   make/scripts/sync_back_from_ibmi.sh "
            },
            {
                "name": "$(telescope) Show Change Object",
				"color": "white",
				"useVsCodeApi": false,
                "command": "make/scripts/cleanup.sh   &&   make/scripts/create_build_script.sh show-change-object"
            }
        ]
        }
}